{
  "type": "sqlite",
  "database": "database/database.sqlite",
  "synchronize": true,
  "logging": ["error"],
  "entities": [
    "src/app/orm/entity/**/*.ts",
    "app/orm/entity/**/*.js"
  ],
  "migrations": [
    "src/app/orm/migration/**/*.ts",
    "app/orm/migration/**/*.js"
  ],
  "subscribers": [
    "src/app/orm/subscriber/**/*.ts",
    "app/orm/subscriber/**/*.js"
  ],
  "cli": {
    "entitiesDir": "src/app/orm/entity",
    "migrationsDir": "src/app/orm/migration",
    "subscribersDir": "src/app/orm/subscriber"
  }
}
