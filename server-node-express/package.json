{
  "private": true,
  "name": "server-node-express",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && gulp copyFiles",
    "prestart": "npm run build",
    "start": "node ./target",
    "predebug": "npm run build",
    "debug": "node --inspect ./target",
    "start:ts": "ts-node ./src",
    "debug:ts": "node --inspect -r ts-node/register ./src",
    "dev": "nodemon",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n generated",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n TODO",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "lint": "tslint -p ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "class-transformer": "^0.1.9",
    "class-transformer-validator": "^0.5.0",
    "class-validator": "^0.8.5",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "elasticsearch": "^15.0.0",
    "escape-html": "^1.0.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "inversify": "^4.13.0",
    "jimp": "^0.2.28",
    "keycloak-connect": "3.4.3",
    "multer": "^1.3.0",
    "npm": "^5.8.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.10",
    "sqlite3": "^4.0.0",
    "typeorm": "^0.2.4",
    "uuid": "^3.2.1",
    "yandex-checkout": "^1.0.10"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/jimp": "^0.2.28",
    "@types/multer": "^1.3.6",
    "@types/node": "^7.0.61",
    "@types/typescript": "^2.0.0",
    "gulp": "^3.9.1",
    "nodemon": "^1.17.3",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^2.8.1"
  }
}
